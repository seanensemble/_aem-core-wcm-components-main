    "teaser": {
        "jcr:primaryType": "sling:Folder",
        "v1"             : {
            "jcr:primaryType": "nt:unstructured",
            "teaser"         : {
                "cq:icon"                : "image",
                "jcr:description"        : "Displays an image together with a link that can act as a teaser for a site section.",
