                    "jcr:primaryType": "cq:ClientLibraryFolder",
                    "allowProxy": true,
                    "categories": [
                        "core.wcm.components.teaser.v1"
                    ],
                    "css.txt": {
                        "jcr:primaryType": "nt:file",
                        "jcr:content": {
                            "jcr:primaryType": "nt:resource",
